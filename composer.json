{
    "name": "w3scout/contao-searchfaq-bundle",
    "homepage": "https://github.com/w3scout/contao-searchfaq-bundle",
    "description":"Search FAQ-Sites.",
    "keywords":["contao", "contao-bundle", "search", "faq", "search faq", "faq-accordion"],
    "type": "contao-bundle",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Darko Selesi",
            "homepage": "https://w3scouts.com",
            "role": "Developer"
        }
    ],
    "support":{
        "issues":"https://github.com/w3scout/contao-searchfaq-bundle/issues",
        "source":"https://github.com/w3scout/contao-searchfaq-bundle"
    },
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^4.9 || ^5.0",
        "contao/faq-bundle": "^4.9 || ^5.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.12"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "W3Scout\\ContaoSearchFaq\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": false,
            "contao/manager-plugin": false,
            "contao-community-alliance/composer-plugin": true
        }
    },
    "extra": {
        "contao-manager-plugin": "W3Scout\\ContaoSearchFaq\\ContaoManager\\Plugin"
    }
}
