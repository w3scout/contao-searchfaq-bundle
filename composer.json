{
  "name": "w3scout/contao-searchfaq-bundle",
  "homepage": "https://github.com/w3scout/contao-searchfaq-bundle",
  "description":"Search FAQ-Sites.",
  "keywords":["contao", "contao4", "contao-bundle", "search", "faq", "faq-accordion"],
  "type": "contao-bundle",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Darko Selesi",
      "homepage": "https://w3scouts.com",
      "role": "Developer"
    }
  ],
  "support":{
    "email":"hallo@w3scouts.com",
    "issues":"https://github.com/w3scout/contao-searchfaq-bundle/issues",
    "source":"https://github.com/w3scout/contao-searchfaq-bundle"
  },
  "require": {
    "php": ">=5.6.0",
    "contao/core-bundle": "^4.4",
    "contao/faq-bundle": "^4.4"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "conflict": {
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "W3Scout\\SearchfaqBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "W3Scout\\SearchfaqBundle\\ContaoManager\\Plugin"
  }
}
